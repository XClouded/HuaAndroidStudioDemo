<?xml version="1.0" encoding="utf-8"?>
<!--返回按钮-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_window_focused="false" android:drawable="@drawable/share_icon1">
    </item>
    <item android:state_focused="true" android:state_pressed="true" >

        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <bitmap android:src="@drawable/share_icon1"
                        android:gravity="center" />
            </item>
            <item android:top="10dp" android:left="10dp">
                <shape>
                    <solid android:color="@color/search_select"/>

                    <stroke android:width="1dp" android:color="@color/gray12"/>
                    <padding android:bottom="@dimen/search_padding_top" android:left="@dimen/search_padding_left"
                             android:right="@dimen/search_padding_left" android:top="@dimen/search_padding_top"/>
                    <corners android:radius="@dimen/choiceInterest_radius"/>
                </shape>
            </item>
        </layer-list>

    </item>
    <item android:state_focused="false" android:state_pressed="true" >

        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <bitmap android:src="@drawable/share_icon1"
                        android:gravity="center" />
            </item>
            <item android:top="10dp" android:left="10dp">
                <shape>
                    <solid android:color="@color/search_select"/>

                    <stroke android:width="1dp" android:color="@color/gray12"/>
                    <padding android:bottom="@dimen/search_padding_top" android:left="@dimen/search_padding_left"
                             android:right="@dimen/search_padding_left" android:top="@dimen/search_padding_top"/>
                    <corners android:radius="@dimen/choiceInterest_radius"/>
                </shape>
            </item>
        </layer-list>

    </item>


    <!--<item android:state_window_focused="false" android:drawable="@drawable/share_icon1"/>-->
    <!--<item android:state_focused="true" android:state_pressed="true" android:drawable="@drawable/share_icon1_on" />-->
    <!--<item android:state_focused="false" android:state_pressed="true" android:drawable="@drawable/share_icon1_on" />-->

</selector>