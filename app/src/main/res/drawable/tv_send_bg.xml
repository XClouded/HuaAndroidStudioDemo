<?xml version="1.0" encoding="utf-8"?>
<!--返回按钮-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
          
      
    <item android:state_focused="true" android:state_pressed="true">
        <shape >

            <gradient
                android:angle="-90"
                android:endColor="@color/green4"
                android:startColor="@color/green5"/>
            <corners android:radius="25dp"/>
            <padding android:bottom="@dimen/square_bg_padding" android:left="@dimen/square_bg_padding"
                     android:right="@dimen/square_bg_padding" android:top="@dimen/square_bg_padding"/>
        </shape>
    </item>

    <item >
        <shape >

            <gradient
                android:angle="-90"
                android:endColor="@color/green5"
                android:startColor="@color/green4"/>
            <corners android:radius="25dp"/>
            <padding android:bottom="@dimen/square_bg_padding" android:left="@dimen/square_bg_padding"
                     android:right="@dimen/square_bg_padding" android:top="@dimen/square_bg_padding"/>
        </shape>
    </item>

</selector>